    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <add name="IDSMContext" connectionString="Data Source=|DataDirectory|\IDSM.sdf" providerName="System.Data.SqlServerCe.4.0" />
    <add name="ErrorLogs" connectionString="Data Source=|DataDirectory|\ErrorLogs.sdf" providerName="System.Data.SqlServerCe.4.0" />
    <add name="ErrorLogsEntities" connectionString="metadata=res://*/Entities.ErrorLogs.csdl|res://*/Entities.ErrorLogs.ssdl|res://*/Entities.ErrorLogs.msl;provider=System.Data.SqlServerCe.4.0;provider connection string=&quot;data source=C:\Projects\idsm_v2_ef6\IDSM\IDSM\App_Data\ErrorLogs.sdf&quot;" providerName="System.Data.EntityClient" />
