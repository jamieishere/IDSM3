    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="DataUploadFileName" value="idsm_subset_3.csv" />
    <add key="AppDataUploadsPath" value="C:/Projects/idsm_v2_ef6/IDSM/IDSM/App_Data/Uploads/" />
